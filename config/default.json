{
  "concurrency": 10,
  "stopPinLimit": 10,
  "minPinThreshold": 5,
  "recentPinCount": 10,
  "resourceLimits": {
    "disk": "10GB"
  },
  "logging": {
    "level": "debug",
    "path": "./pinbot.log"
  },
  "IPFSNode": {
    "host": "localhost",
    "port": 5001,
    "protocol": "http"
  },
  "pinbots": [
